{
  "styles": {
    "libs": {
      "src": [
        "lib/bootstrap/dist/css/bootstrap.css"
      ],
      "dst": [
        "styles/libs.dist.css"
      ]
    },
    "main": {
      "src": [
        "styles/css/main.css"
      ],
      "dst": [
        "styles/main.dist.css"
      ]
    }
  },
  "js": {
    "libs": {
      "src": [
        "lib/jquery/dist/jquery.js",
        "lib/bootstrap/dist/js/bootstrap.js",
        "lib/handlebars/handlebars.js",
        "lib/ember/ember.js",
        "lib/ember-data/ember-data.js",
        "lib/ember-localstorage-adapter/localstorage_adapter.js"
      ],
      "dst": [
        "js/libs.dist.js"
      ]
    },
    "app": {
      "src": [
        "js/app-init.js",
        "js/store.js",
        "js/router.js",
        "js/routes/*.js",
        "js/models/*.js",
        "js/helpers/*.js",
        "js/controllers/*.js"
      ],
      "dst": ["js/app.dist.js"]
    },
    "templates": {
      "src": ["js/templates.js"],
      "dst": ["js/templates.dist.js"]
    }
  },
  "img": {
    "favicon": {
      "src": ["img/favicon.ico"],
      "dst": ["favicon.ico"]
    },
    "images": {
      "src": ["img/**/*.jpg"],
      "dst": ["img"]
    }
  }
}
